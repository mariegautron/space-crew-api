{
  "name": "space-crew-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p . && node_modules/cpy-cli/cli.js 'src/**/*.json' dist/src",
    "init:prima": "npx prisma init",
    "db:pull": "npx prisma db pull",
    "db:reset": "npx prisma migrate reset",
    "db:migrate": "npx prisma migrate dev",
    "fixtures": "npx prisma db seed",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "clean:code": "npm run format && npm run pretty",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cpy": "^10.0.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.40.0",
    "husky": "^4.3.8",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean:code"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run clean:code",
      "git add"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seeds/index.ts"
  },
  "engines": {
    "node": ">=14.18"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.ts"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.3"
  }
}
